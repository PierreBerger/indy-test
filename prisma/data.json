[
  {
    "name": "WeatherCode",
    "avantage": { "percent": 20 },
    "restrictions": [
      { "@date": { "after": "2020-01-01", "before": "2024-06-30" } },
      {
        "@or": [
          { "@age": { "eq": 40 } },
          {
            "@and": [
              { "@age": { "lt": 30, "gt": 15 } },
              { "@meteo": { "is": "clear", "temp": { "gt": "15" } } }
            ]
          }
        ]
      }
    ]
  },
  {
    "name": "FEB80",
    "avantage": { "percent": 80 },
