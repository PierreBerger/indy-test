              { "@age": { "lt": 100, "gt": 10 } },
              { "@meteo": { "is": "clear", "temp": { "gt": "15" } } }
            ]
          }
        ]
      },
      {
        "@or": [
          { "@age": { "eq": 35 } },
          {
            "@or": [
              { "@meteo": { "is": "dust", "temp": { "lt": "40" } } },
